variable:    NAMES ARE y x y1 x1 t c t2;
             usevar=y x;
             cluster=c;
             lagged=y(1) x(1);
             missing=all(999);
             tinterval=t(100);

data:file=dlist.dat; type=montecarlo;

ANALYSIS: 	estimator=bayes;  TYPE IS TWOLEVEL; proc=2;

MODEL:

  %WITHIN%
  y*1 x*1;
  y^ on y^1*0.7;
  y on x*1;
  x^ on x^1*0.7;

  %between%
  y*1 x*1; y on x*-1;

OUTPUT: TECH8;
